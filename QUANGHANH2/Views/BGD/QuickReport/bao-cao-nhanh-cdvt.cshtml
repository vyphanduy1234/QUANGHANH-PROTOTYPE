                        <h6 class="card-subtitle">Bảo dưỡng </h6>
                        <h4> 1000</h4>
                    </div>

                    <div class="col s12">
                        <div class="progress">
                            <div class="determinate amber darken+5" style="width: 100%"></div>
                        </div>
                    </div>

                    <div class="col s12 center-align">
                        <h6 class="card-subtitle">Chi phí (triệu đồng)</h6>
                        <h4 class="red-text font-medium m-b-0"> -1000 </h>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="col s12 l2">
        <div class="card card-hover" style="height: 200px">
            <div class="card-content align-items-center table_container">
                <div class="row">
                    <div class="col s12">
                        <h6 class="card-subtitle">Kiểm định</h6>
                        <h4> 1000</h4>
                    </div>

                    <div class="col s12">
                        <div class="progress">
                            <div class="determinate amber darken+5" style="width: 100%"></div>
                        </div>
                    </div>

                    <div class="col s12 center-align">
                        <h6 class="card-subtitle">Chi phí (triệu đồng)</h6>
                        <h4 class="red-text font-medium m-b-0"> -1000 </h>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="col s12 l2">
        <div class="card card-hover" style="height: 200px">
            <div class="card-content align-items-center table_container">
                <div class="row">
                    <div class="col s12">
                        <h6 class="card-subtitle">Thanh lý</h6>
                        <h4> 1000</h4>
                    </div>

                    <div class="col s12">
                        <div class="progress">
                            <div class="determinate amber darken+5" style="width: 100%"></div>
                        </div>
                    </div>

                    <div class="col s12 center-align">
                        <h6 class="card-subtitle">Tiền (triệu đồng)</h6>
                        <h4 class="green-text font-medium m-b-0"> +1000 </h>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="col s12 l2">
        <div class="card card-hover" style="height: 200px">
            <div class="card-content align-items-center table_container">
                <div class="row">
                    <div class="col s12 center-align" style="padding-top: 45px">
                        <h6 class="card-subtitle">Thu hồi</h6>
                        <h4 class="font-medium m-b-0"> 1000 </h>
                    </div>
                </div>
            </div>
        </div>
    </div>

</div>


<!-- Start Pie Chart -->
