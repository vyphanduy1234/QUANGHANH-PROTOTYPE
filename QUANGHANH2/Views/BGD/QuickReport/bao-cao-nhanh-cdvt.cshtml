<div class="row">
    <div class="col s12 l12">
        <div class="card">
            <div class="card-content center-align">
                <h3 class="center-align m-b-0"><b>LƯỢNG THIẾT BỊ TRONG THÁNG</b></h3>
            </div>
        </div>
    </div>
</div>

<div class="row center-align">


    <div class="col s12 l2">
        <div class="card mymanu card-hover" style="height: 200px">
            <div class="card-content center-align table_container">
                <h6 class="card-subtitle center-align m-b-0 white-text">Tổng</h6>
                <h1 class="font-light font-medium m-b-0 white-text" style="padding-top: 40px"> 12000 </h1>
            </div>
        </div>
    </div>

    <div class="col s12 l2">
        <div class="card card-hover" style="height: 200px">
            <div class="card-content align-items-center table_container">
                <div class="row">
                    <div class="col s12">
                        <h6 class="card-subtitle">Sửa chữa</h6>
                        <h4> 1000</h4>
                    </div>

                    <div class="col s12">
                        <div class="progress">
                            <div class="determinate amber darken+5" style="width: 100%"></div>
                        </div>
                    </div>

                    <div class="col s12 center-align">
                        <h6 class="card-subtitle">Chi phí (triệu đồng)</h6>
                        <h4 class="red-text font-medium m-b-0"> -1000 </h>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="col s12 l2">
        <div class="card card-hover" style="height: 200px">
            <div class="card-content align-items-center table_container">
                <div class="row">
